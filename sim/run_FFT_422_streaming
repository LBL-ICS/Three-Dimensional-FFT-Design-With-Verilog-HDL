add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/TwiddleFactorsStreamed/io_out_1_Im

add wave -position insertpoint /FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/RAM_Block_clock
add wave -position insertpoint /FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming_2/RAM_Block_3/clock
add wave -position insertpoint /FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/RAM_Block_io_in_raddr
