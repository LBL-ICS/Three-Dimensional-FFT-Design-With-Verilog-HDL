#!/bin/csh -f
################################################################################
# simulation top script
# change modelsim in this file dir and run "do this_file.do"
# project dir ---+---> hdl source dir
#                +---> sim script dir
#
################################################################################
# check if current dir has modelsim config file
set has_config  [file exists modelsim.do]
# config modelsim 
if {$has_config==1} { do modelsim.do  ; }

echo "+====================================="
echo "| Creat Lib Work soc                  "
echo "+====================================="
vlib work
vmap work work 

echo "+====================================="
echo "| Complile RTL Code of soc            "
echo "+====================================="
vlog +define+FFT_422_STREAMING\
     -f ../filelist/filelist_FFT_422_streaming.f

echo "+====================================="
echo "| Compiler Pass                       "
echo "| Being to Run Simulation             "
echo "+====================================="
vsim work.FFT_tb -t 1ns

###################################### mus  ####################################
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/clock
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/reset
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_in_ready
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_in_0_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_in_0_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_in_1_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_in_1_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_out_validate
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_out_0_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_out_0_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_out_1_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/io_out_1_Im

add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/clock
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/reset
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/io_in_0_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/io_in_0_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/io_in_1_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/io_in_1_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/io_in_en_0
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_422_streaming/PermutationsWithStreaming/io_in_en_1
