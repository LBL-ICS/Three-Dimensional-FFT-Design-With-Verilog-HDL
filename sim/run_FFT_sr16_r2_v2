#!/bin/csh -f
################################################################################
# simulation top script
# change modelsim in this file dir and run "do this_file.do"
# project dir ---+---> hdl source dir
#                +---> sim script dir
#
################################################################################
# check if current dir has modelsim config file
set has_config  [file exists modelsim.do]
# config modelsim 
if {$has_config==1} { do modelsim.do  ; }

echo "+====================================="
echo "| Creat Lib Work soc                  "
echo "+====================================="
vlib work
vmap work work 

echo "+====================================="
echo "| Complile RTL Code of soc            "
echo "+====================================="
vlog +define+FFT_SR16_R2\
     +define+FFT_SR16_R2_V2\
     -f ../filelist/filelist_FFT_sr16_r2_v2.f

echo "+====================================="
echo "| Compiler Pass                       "
echo "| Being to Run Simulation             "
echo "+====================================="
vsim work.FFT_tb -t 1ns

###################################### mus  ####################################
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/clock
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/reset
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_ready
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_0_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_0_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_1_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_1_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_2_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_2_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_3_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_3_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_4_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_4_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_5_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_5_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_6_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_6_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_7_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_7_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_8_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_8_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_9_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_9_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_10_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_10_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_11_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_11_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_12_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_12_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_13_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_13_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_14_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_14_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_15_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_in_15_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_validate
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_0_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_0_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_1_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_1_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_2_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_2_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_3_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_3_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_4_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_4_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_5_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_5_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_6_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_6_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_7_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_7_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_8_Re 
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_8_Im 
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_9_Re 
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_9_Im 
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_10_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_10_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_11_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_11_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_12_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_12_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_13_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_13_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_14_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_14_Im
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_15_Re
add wave -noupdate -format logic -radix hexadecimal FFT_tb/u_FFT_sr16_r2_v2/io_out_15_Im

add wave -position insertpoint /FFT_tb/io_in_9_Im
add wave -position insertpoint /FFT_tb/io_in_10_Re
add wave -position insertpoint /FFT_tb/io_in_10_Im
add wave -position insertpoint /FFT_tb/io_in_11_Re
add wave -position insertpoint /FFT_tb/io_in_11_Im
add wave -position insertpoint /FFT_tb/io_in_12_Re
add wave -position insertpoint /FFT_tb/io_in_12_Im
add wave -position insertpoint /FFT_tb/io_in_13_Re
add wave -position insertpoint /FFT_tb/io_in_13_Im
add wave -position insertpoint /FFT_tb/io_in_14_Re
add wave -position insertpoint /FFT_tb/io_in_14_Im
add wave -position insertpoint /FFT_tb/io_in_15_Re
add wave -position insertpoint /FFT_tb/io_in_15_Im


add wave -position insertpoint /FFT_tb/u_FFT_sr16_r2_v2/PermutationsBasic_4_io_out_0_Re
add wave -position insertpoint /FFT_tb/u_FFT_sr16_r2_v2/PermutationsBasic_4_io_out_0_Im
add wave -position insertpoint /FFT_tb/u_FFT_sr16_r2_v2/PermutationsBasic_4/io_out_0_Im
add wave -position insertpoint /FFT_tb/u_FFT_sr16_r2_v2/results_0_Re
add wave -position insertpoint /FFT_tb/u_FFT_sr16_r2_v2/regDelays_7
add wave -position insertpoint /FFT_tb/u_FFT_sr16_r2_v2/PermutationsBasic_4_io_out_0_Re


add wave -position insertpoint /FFT_tb/u_FFT_sr16_r2_v2/results_0_Re
add wave -position insertpoint /FFT_tb/u_FFT_sr16_r2_v2/io_out_0_Re

run 1ms                                                                             
                                                                                     
                                                                                     
                                                                                     
                                                                                     
                                                                                     
                                                                                     
                                                                                     
                                                                                     
                                                                                     
                                                                                     
                                                                                     
                                                                                     
